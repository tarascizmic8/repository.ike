<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">10</defaultcontrol>
	
    <controls>
        <!--Dialog Overlay-->
        <include>DialogOverlayExtra</include>
        <!--Dialog OK-->
        <control type="group">
            <include>animation_window_open_close</include>
			<control type="image">
				<posx>0</posx>
				<posy>315</posy>
				<width>100%</width>
				<height>450</height>
				<texture border="5">diffuse/panel.png</texture>
				<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
			</control>
            <control type="image">
				<posx>0</posx>
				<posy>320</posy>
				<width>100%</width>
				<height>440</height>
				<texture border="5">diffuse/panel.png</texture>
				<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
			</control>
			<control type="button" id="10">
				<description>Ok button</description>
				<width>300</width>
				<height>90</height>
				<left>810</left>
				<posy>630</posy>
				<font>Reg36</font>
				<include>DialogButtonOther</include>
				<label>186</label>
			</control>
			<control type="grouplist">
				<posx>0</posx>
				<posy>400</posy>
				<width>100%</width>
				<control type="label" id="1">
					<description>Dialog Header</description>
					<width>100%</width>
					<height>60</height>
					<font>Bold55</font>
					<align>center</align>
					<textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
				</control>
				<control type="textbox" id="9">
					<description>Dialog Header</description>
					<width>100%</width>
					<height>150</height>
					<align>center</align>
					<aligny>top</aligny>
					<include>DialogHeader2</include>
					<!--Doubles Line-->
				</control>
			 </control>
        </control>
    </controls>
</window>