<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">28</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <controls>
        <!--Dialog Overlay-->
		<control type="group">
			<visible>!window.isactive(fullscreenvideo)</visible>
			<include>DialogOverlayExtra</include>
			<animation effect="fade" time="250" start="0" end="100" reversible="false">WindowOpen</animation>
        </control>

        <control type="group">
			<animation effect="fade" end="0" time="0" reversible="false" condition="Window.IsVisible(yesnodialog) | Window.IsVisible(addonsettings)">Conditional</animation>

			<animation type="WindowOpen">
				<effect type="slide" start="1200" end="0" time="250" tween="cubic" easing="inout" />
			</animation>
			<animation type="WindowClose">
				<effect type="slide" start="0" end="1200" time="250" tween="cubic" easing="inout" />
			</animation>
            <!--Panel-->
             <!--Panel-->
            <control type="image">
                <posx>630</posx>
                <posy>0</posy>
                <width>1340</width>
                <height>100%</height>
				<texture border="15">diffuse/bgpanel.png</texture>
				
				<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
				<visible>!skin.hastheme(classic)</visible>
            </control>
            <!-- close dialog -->
            <control type="group">
                <include>animation_fade_visible_hidden</include>
                <right>60</right>
                <width>60</width>
                <posy>40</posy>
                <include condition="Skin.HasSetting(EnableTouchSupport)">close_button</include>
            </control>
			
           <!--Header-->
            <control type="label" id="1">
                <description>Heading</description>
                <posx>700</posx>
                <posy>10</posy>
                <width>1200</width>
				<font>Bold45</font>
				<textcolor>$INFO[Skin.String(GeneralHighlightTextColor)]</textcolor>
                <align>left</align>
				<height>100</height>
				<label>20333</label>
            </control>
			
			<control type="group" id="8001">
				<control type="spincontrolex" id="20">
					<description>Directory Content</description>
					<posx>680</posx>
					<posy>150</posy>
					<width>1200</width>
					<label>20344</label>
					<align>left</align>
					<texturenofocus>-</texturenofocus>
					<include>DialogButton</include>
					<textoffsetx>20</textoffsetx>
					<spinposx>5</spinposx>
					<onup>8002</onup>
					<ondown>21</ondown>
					<onleft>8002</onleft>
					<onright>8002</onright>
				</control>

				
				<control type="image">
					<posx>680</posx>
					<posy>220</posy>
					<width>1200</width>
					<height>350</height>
					<texture border="5">dialogs/default/inner.png</texture>
				</control>
				
				<!--Icon-->
				<control type="image" id="3">
					<right>15</right>
					<posy>235</posy>
					<width>350</width>
					<height>300</height>
					<fadetime>800</fadetime>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<texture diffuse="diffuse/panel.png">$INFO[ListItem.Icon]</texture>
				</control>

				<control type="list" id="21">
					<onup>20</onup>
					<posx>680</posx>
					<posy>230</posy>
					<width>800</width>
					<height>303</height>
					<onup>3</onup>
					<onleft>8002</onleft>
					<onright>60</onright>
					<ondown>5</ondown>
					<pagecontrol>60</pagecontrol>
					<scrolltime tween="cubic" easing="out">500</scrolltime>
					<itemlayout height="60">
						<control type="image">
							<posx>0</posx>
							<posy>1</posy>
							<width>768</width>
							<height>60</height>
							<texture>dialogs/default/separator.png</texture>
						</control>
						<control type="image">
							<posx>20</posx>
							<posy>10</posy>
							<width>40</width>
							<height>40</height>
							<info>ListItem.Thumb</info>
						</control>
						<control type="label">
							<posx>75</posx>
							<posy>0</posy>
							<width>633</width>
							<height>60</height>
							<include>DialogListNF</include>
							<info>ListItem.Label</info>
							<align>left</align>
							<aligny>center</aligny>
							<scroll>false</scroll>
						</control>
						<control type="image">
							<posx>718</posx>
							<posy>10</posy>
							<width>40</width>
							<height>40</height>
							<texture>common/checkbox.png</texture>
							<colordiffuse>$INFO[Skin.String(ViewDetailsTextColor)]</colordiffuse>
							<visible>ListItem.IsSelected</visible>
						</control>
					</itemlayout>
					<focusedlayout height="60">
						<control type="image">
							<posx>0</posx>
							<posy>1</posy>
							<width>768</width>
							<height>60</height>
							<texture>dialogs/default/separator.png</texture>
						</control>
						<control type="image">
							<width>768</width>
							<height>60</height>
							<texture border="5">diffuse/panel.png</texture>
							<colordiffuse>$INFO[Skin.String(ButtonFocusColor)]</colordiffuse>
							<visible>Control.HasFocus(4) | Control.HasFocus(21)</visible>
						</control>
						<control type="image">
							<posx>20</posx>
							<posy>10</posy>
							<width>40</width>
							<height>40</height>
							<info>ListItem.Thumb</info>
						</control>
						<control type="label">
							<posx>75</posx>
							<posy>0</posy>
							<width>633</width>
							<height>60</height>
							<include>DialogListNF</include>
							<info>ListItem.Label</info>
							<align>left</align>
							<aligny>center</aligny>
							<scroll>false</scroll>
							<visible>!Control.HasFocus(4) + !Control.HasFocus(21)</visible>
						</control>
						<control type="label">
							<posx>75</posx>
							<posy>0</posy>
							<width>633</width>
							<height>60</height>
							<textcolor>black</textcolor>
							<selectedcolor>black</selectedcolor>
							<include>DialogListNF</include>
							<info>ListItem.Label</info>
							<align>left</align>
							<aligny>center</aligny>
							<scroll>false</scroll>
							<visible>Control.HasFocus(4) | Control.HasFocus(21)</visible>
						</control>
						<control type="image">
							<posx>718</posx>
							<posy>10</posy>
							<width>40</width>
							<height>40</height>
							<texture>common/checkbox.png</texture>
							<colordiffuse>$INFO[Skin.String(ViewDetailsTextColor)]</colordiffuse>
							<visible>ListItem.IsSelected</visible>
						</control>
					</focusedlayout>
				</control>
				
				<control type="image" id="62">
					<animation effect="fade" start="100" end="0" time="0" delay="1500">WindowOpen</animation>
				</control>
				<control type="image" id="61">
					<animation effect="fade" start="0" end="100" time="0">Visible</animation>
					<animation effect="fade" start="100" end="0" time="0">Hidden</animation>
					<visible>Control.HasFocus(60) | !Control.IsVisible(62)</visible>
				</control>
				<control type="image" id="61">
					<animation effect="fade" start="0" end="100" time="0">Visible</animation>
					<animation effect="fade" start="100" end="0" time="0" delay="1000">Hidden</animation>
					<visible>!Control.HasFocus(60) + [Container(4).Scrolling | Container(4).OnScrollNext | Container(4).OnScrollPrevious]</visible>
				</control>
				
				<control type="image">
					<posx>680</posx>
					<posy>600</posy>
					<width>1200</width>
					<height>300</height>
					<texture border="5">dialogs/default/inner.png</texture>
				</control>
				<control type="grouplist" id="5">
                    <description>Control Area</description>
                    <posx>680</posx>
                    <posy>600</posy>
                    <width>1200</width>
                    <height>300</height>
                    <orientation>vertical</orientation>
                    <itemgap>0</itemgap>
					<onup>21</onup>
                    <ondown>8002</ondown>
                    <onleft>8002</onleft>
                    <onright>8002</onright>
                    <scrolltime tween="cubic" easing="out">500</scrolltime>
                </control>
                <control type="button" id="7">
                    <description>default</description>
					<width>1200</width>
                    <height>60</height>
                    <align>left</align>
                    <textoffsetx>20</textoffsetx>
					<texturenofocus>dialogs/default/separator.png</texturenofocus>
                    <include>DialogButton</include>
                </control>
                <control type="radiobutton" id="8">
                    <description>radioButton</description>
					<width>1200</width>
                    <height>60</height>
                    <align>left</align>
                    <textoffsetx>20</textoffsetx>
					<texturenofocus>dialogs/default/separator.png</texturenofocus>
					<include>DialogButton</include>
                    <radioposx>1080</radioposx>
                </control>
			
			
			</control>
			
            <!--Button Group-->
            <control type="grouplist" id="8002">
                <posx>680</posx>
                <posy>940</posy>
				<orientation>horizontal</orientation>
				<itemgap>20</itemgap>
				<onup>5</onup>
				<control type="button" id="28">
                    <description>OK</description>
                    <label>186</label>
                    <include>DialogButtonOther</include>
                    <animation effect="fade" start="100" end="75" time="0" condition="!Control.IsEnabled(28)">Conditional</animation>
                </control>
                <control type="button" id="29">
                    <posx>200</posx>
                    <description>Cancel</description>
                    <label>$LOCALIZE[222]</label>
					<include>DialogButtonOther</include>
                    <animation effect="fade" start="100" end="75" time="0" condition="!Control.IsEnabled(29)">Conditional</animation>
                </control>
				<control type="button" id="22">
                    <posx>740</posx>
                    <description>Settings</description>
                    <label>$LOCALIZE[5]</label>
					<include>DialogButtonOther</include>
                    <animation effect="fade" start="100" end="75" time="0" condition="!Control.IsEnabled(6)">Conditional</animation>
                </control>
            </control>
        </control>
    </controls>
</window>